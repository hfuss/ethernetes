  "widgets": [
    {
      "id": 6108504807217558,
      "definition": {
        "title": "Miner Uptime",
        "type": "group",
        "layout_type": "ordered",
        "widgets": [
          {
            "id": 4282084101506516,
            "definition": {
              "title": "Is Miner Running?",
              "title_size": "16",
              "title_align": "left",
              "show_legend": false,
              "type": "timeseries",
              "requests": [
                {
                  "q": "min:docker.containers.running{image_name:ghcr.io/hfuss/miner,$host} by {host}",
                  "on_right_yaxis": false,
                  "style": {
                    "palette": "dog_classic",
                    "line_type": "solid",
                    "line_width": "normal"
                  },
                  "display_type": "area"
                }
              ],
              "yaxis": {
                "scale": "linear",
                "label": "",
                "include_zero": true,
                "min": "auto",
                "max": "auto"
              },
              "markers": []
            }
          },
          {
            "id": 983337016343540,
            "definition": {
              "title": "Did Miners Have 99.9% Uptime?",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "q": "avg:docker.containers.running{$host,image_name:ghcr.io/hfuss/miner}*100",
                  "aggregator": "avg",
                  "conditional_formats": [
                    {
                      "hide_value": true,
                      "custom_fg_color": "#eb0a0a",
                      "comparator": "<",
                      "palette": "white_on_red",
                      "value": 99.9
                    },
                    {
                      "comparator": ">=",
                      "palette": "white_on_green",
                      "value": 99.9
                    }
                  ]
                }
              ],
              "autoscale": false,
              "custom_unit": "%",
              "precision": 2
            }
          }
        ]
      }
    },
